<?xml version="1.0" encoding="ISO-8859-1"?>

<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="beans.xsd">
    <bean class="questions.data.QuestionsData">
        <property name="question" type="String"/>
        <property name="answer" type="String"/>
        <property name="questionCustomerName" type="String"/>
        <property name="answerCustomerName" type="String"/>
    </bean>
    <bean class="de.hybris.platform.commercefacades.product.data.ProductData">
        <property name="questions" type="java.util.List&lt;questions.data.QuestionsData&gt;"/>
        <property name="questionsCount" type="java.lang.Integer"/>
    </bean>
</beans>
