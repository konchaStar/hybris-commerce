<?xml version="1.0" encoding="ISO-8859-1"?>
<items 	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
			xsi:noNamespaceSchemaLocation="items.xsd">
	<relations>
		<relation code="Product2Question" localized="false">
			<sourceElement type="Product" qualifier="product" cardinality="one">
				<modifiers read="true" write="true" search="true" optional="true"/>
			</sourceElement>
			<targetElement type="Question" qualifier="questions" cardinality="many" collectiontype="list">
				<modifiers read="true" write="true" search="true" optional="true"/>
			</targetElement>
		</relation>
	</relations>

	<itemtypes>
		<itemtype code="Question" generate="true" autocreate="true">
			<deployment table="questions" typecode="20201"/>
			<attributes>
				<attribute qualifier="code" type="java.lang.String">
					<persistence type="property"/>
				</attribute>
				<attribute qualifier="question" type="java.lang.String">
					<persistence type="property"/>
				</attribute>
				<attribute qualifier="questionCustomer" type="Customer">
					<persistence type="property"/>
				</attribute>
				<attribute qualifier="answer" type="java.lang.String">
					<persistence type="property"/>
				</attribute>
				<attribute qualifier="answerCustomer" type="Customer">
					<persistence type="property"/>
				</attribute>
			</attributes>
		</itemtype>

		<itemtype code="QuestionsCMSComponent" extends="AbstractCMSComponent" generate="true" autocreate="true"
			abstract="false" jaloclass="com.es.questions.jalo.QuestionsCMSComponent">
			<attributes>
				<attribute qualifier="numberOfQuestionsToShow" type="java.lang.Integer">
					<persistence type="property"/>
				</attribute>
				<attribute qualifier="fontSize" type="java.lang.Integer">
					<persistence type="property"/>
				</attribute>
			</attributes>
		</itemtype>

		<itemtype code="Product" autocreate="false" generate="true">
			<attributes>
				<attribute qualifier="warrantyYears" type="java.lang.Integer">
					<persistence type="property"/>
				</attribute>
			</attributes>
		</itemtype>

		<itemtype code="WarrantyCMSComponent" extends="AbstractCMSComponent" generate="true" autocreate="true"
				  abstract="false" jaloclass="com.es.questions.jalo.WarrantyCMSComponent"/>
	</itemtypes>
</items>
